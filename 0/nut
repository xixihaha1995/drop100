    {
        type            nutkWallFunction;
        value           uniform 0;
    }
    TOP
    {
        type            calculated;
        value           uniform 0;
    }
    WALL1
    {
        type            calculated;
        value           uniform 0;
    }
    WALL2
    {
        type            calculated;
        value           uniform 0;
    }
    WALL3
    {
        type            calculated;
        value           uniform 0;
    }
    WALL4
    {
        type            calculated;
        value           uniform 0;
    }
}


